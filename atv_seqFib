#include <stdio.h>

int SeqFib(int n){ // n representa a posição
    if(n==0){
        return 1;
    } else if(n==1){
        return 1;
    } else{
        return SeqFib(n-1) + SeqFib(n-2);
    }
}
int main()
{
    int i, qntd;
    printf("Informe a quantidade de termos:\n");
    scanf("%i", &qntd);
    
    //Imprimindo a sequencia de fibonacci...
    for(i=0;i<qntd;i++){
        printf("[%i]\t", i);
        printf("%i\n", SeqFib(i));
    }

    return 0;
